\ProvidesPackage{beamerthemescilife}[2014/10/01 v1.1 for beamer 3.01]
\mode<presentation>

\RequirePackage{tikz}

% Setup fonts
\usefonttheme{scilife}

% Setup colors
\usecolortheme{scilife}

% Setup structure
\useinnertheme{scilife}
\useoutertheme{scilife}

% Additional modifications
\setbeamertemplate{itemize item}[circle]
\setbeamertemplate{itemize subitem}[dash]
\setbeamertemplate{blocks}[rounded][shadow=false]

\mode<all>
